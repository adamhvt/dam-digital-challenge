{
  "version": "1.0.0",
  "name": "rethink",
  "private": true,
  "gulp": {
    "src": {
      "js": "content/stylesheets/vendors",
      "scss": "content/sass",
      "cshtml": "./",
      "dist": "content"
    },
    "selectors": {
      "all": "*",
      "js": "**/*.js",
      "css": "**/*.css",
      "scss": "**/*.scss",
      "cshtml": "**/*.html",
      "images": "**/*.{png,jpg,jpeg,gif,ico,svg}",
      "fonts": "**/*.{eot,svg,ttf,woff,woff2}"
    },
    "main": {
      "server": "index.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.26.3",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-bundle-assets": "^2.29.0",
    "gulp-clean-css": "^4.0.0",
    "gulp-purge-sourcemaps": "^1.0.0",
    "gulp-purgecss": "^1.1.1",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-strip-comments": "^2.5.2",
    "kss": "^3.0.0-beta.25",
    "lazypipe": "^1.0.2",
    "onchange": "^5.2.0",
    "require-dir": "^1.2.0"
  },
  "scripts": {
    "kssWatch": "onchange \"Content/scss/**/*.scss\" -- npm run kssBuild",
    "kssBuild": "kss --config Content/scss/kss-config.json",
    "gulp": "gulp"
  }
}
